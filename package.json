{
  "name": "vending-machine",
  "type": "module",
  "version": "1.0.0",
  "description": "A basic vending machine backend",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "build": "dist/esm/index.d.ts",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src/**/*.d.ts",
    "README.md"
  ],
  "directories": {
    "test": "src/tests"
  },
  "scripts": {
    "dev": "node --inspect dist/esm/index.js",
    "start": "bash ./init.sh",
    "build": "rm -rf dist && prettier --write src/main && yarn run build:esm && yarn run build:cjs",
    "build:esm": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig-cjs.json && tsc-alias -p tsconfig-cjs.json --outDir dist/cjs",
    "clean:install": "rm -rf node_modules && yarn install",
    "lint": "prettier --write src/main && eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Anietie Asuquo <hello@anietieasuquo.com>",
  "license": "MIT",
  "keywords": [],
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.11.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.10",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "tsc-alias": "1.8.8",
    "tsx": "^4.7.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@node-oauth/express-oauth-server": "4.0.0",
    "body-parser": "1.20.2",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "jsonwebtoken": "^9.0.2",
    "tspa": "/Users/anietie/dev/personal/tspa"
  }
}
