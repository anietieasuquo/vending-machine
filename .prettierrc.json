{
  "parser": "typescript",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "arrowParens": "always",
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "bracketSameLine": true,
  "useTabs": false,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "singleAttributePerLine": false,
  "embeddedLanguageFormatting": "auto",
  "quoteProps": "as-needed"
}
